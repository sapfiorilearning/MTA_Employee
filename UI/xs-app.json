{
	"welcomeFile": "/UI/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/UI/(.*)$",
		"target": "$1",
		"localDir": "webapp"
	}, {
		"source": "^/employees.svc/.*$",
		"destination": "java_be",
		"authenticationType": "xsuaa",
		"csrfProtection" : false
	}]
}